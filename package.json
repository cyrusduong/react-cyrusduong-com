    "prettier": "^1.14.0",
    "react-hot-loader": "^4.3.4",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "babel-loader": "^7.1.5",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "semantic-ui-react": "^0.82.1",
    "style-loader": "^0.21.0",
